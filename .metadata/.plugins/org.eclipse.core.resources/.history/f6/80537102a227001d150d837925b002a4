package ch7;

class Car {
	String color;
	int door;
	
	void drive() {
		System.out.println("drive, Brrrrr...");
	}
	void stop() {
		System.out.println("STOP!!");
	}
}

class FireEngine extends Car{
	void water() {
		System.out.println("warter!!!!");
	}
}

public class Ex7_7 {

	public static void main(String[] args) {
		Car car = null;
		FireEngine fe = new FireEngine(); // 인스턴스는 FireEngine임을 기억하
		FireEngine fe2 = null;

	}

}
